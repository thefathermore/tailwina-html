{
  "name": "create-tailwina-html",
  "version": "1.0.0",
  "description": "A modern HTML starter template with Tailwind CSS, Vite, and Handlebars",
  "type": "module",
  "bin": {
    "create-tailwina-html": "./bin/cli.js"
  },
  "scripts": {
    "dev": "cross-env vite",
    "build": "cross-env vite build",
    "preview": "cross-env vite preview",
    "format": "cross-env prettier --write .",
    "optim:image": "cross-env node scripts/optimize-images.js",
    "optim:video": "cross-env node scripts/optimize-videos.js",
    "optim:media": "pnpm run optim:image && pnpm run optim:video",
    "generate:favicon": "cross-env node scripts/generate-favicon.js",
    "generate:placeholder": "cross-env node scripts/generate-placeholder.js",
    "postinstall": "pnpm rebuild sharp || true"
  },
  "keywords": [
    "html",
    "tailwindcss",
    "vite",
    "handlebars",
    "starter",
    "template"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@realfavicongenerator/generate-favicon": "^0.4.16",
    "@realfavicongenerator/image-adapter-node": "^0.4.16",
    "cross-env": "^10.0.0",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "fs-extra": "^11.3.1",
    "globby": "^14.1.0",
    "handlebars-helpers": "^0.10.0",
    "prettier": "^3.6.2",
    "sharp": "^0.34.3",
    "svgo": "^4.0.0",
    "vite": "^7.1.3",
    "vite-plugin-handlebars": "^2.0.0"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.12",
    "kolorist": "^1.8.0",
    "minimist": "^1.2.8",
    "prompts": "^2.4.2",
    "tailwindcss": "^4.1.12"
  },
  "packageManager": "pnpm@10.28.0+sha512.05df71d1421f21399e053fde567cea34d446fa02c76571441bfc1c7956e98e363088982d940465fd34480d4d90a0668bc12362f8aa88000a64e83d0b0e47be48"
}
